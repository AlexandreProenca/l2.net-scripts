// Handle UserInfo packets.

FUNCTION Packet::UserInfo 1 _bUserInfo

  DEFINE SORTEDLIST UserInfo_SL 0
  
  LOCK AEGIS.MUTEX.ENTITYTRACKING.CHARACTER
    GET_TIME AEGIS.ENTITYTRACKING.CHARACTER.UI_TIME
    CALL_EXTERN "DLIB\Packets\ReadUserInfo.l2s" DLIB::Packets::ReadUserInfo UserInfo_SL 1 _bUserInfo
    "AEGIS.ENTITYTRACKING.CHARACTER.UserInfo" = UserInfo_SL.CLONE
  UNLOCK AEGIS.MUTEX.ENTITYTRACKING.CHARACTER
  
RETURN VOID