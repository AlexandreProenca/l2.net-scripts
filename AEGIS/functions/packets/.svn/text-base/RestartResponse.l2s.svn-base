// Handle RestartResponse Packet

FUNCTION Packet::RestartResponse
  
  DEFINE INT ALLOWED 0
  
  PACKET.READ_BYTE NULL
  PACKET.READ_INT32 ALLOWED
  
  IF ALLOWED == TRUE
    IF AEGIS.MUTEX.PIPELINE.NETWORK IS_LOCKED
    ELSE
      LOCK AEGIS.MUTEX.PIPELINE.NETWORK
      AEGIS.ENTITYTRACKING.SKILLS.CLEAR
    ENDIF

  ELSE
    IF AEGIS.MUTEX.PIPELINE.NETWORK IS_LOCKED
      UNLOCK AEGIS.MUTEX.PIPELINE.NETWORK
    ENDIF
  ENDIF

RETURN VOID