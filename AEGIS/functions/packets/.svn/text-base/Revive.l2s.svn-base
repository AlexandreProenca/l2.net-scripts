// Handle Revive packets.


FUNCTION Packet::Revive 1 _bRevive

  DEFINE INT OBJECT_ID
  
  _bRevive.READ_BYTE NULL
  _bRevive.READ_INT32 OBJECT_ID
  
  
  LOCK AEGIS.MUTEX.ENTITYTRACKING.PLAYERS
  IF "AEGIS.ENTITYTRACKING.PLAYERS.CONTAINS_KEY <&OBJECT_ID&>" == TRUE
    "AEGIS.ENTITYTRACKING.PLAYERS.#$<&OBJECT_ID&>.ALIVE" = TRUE
  ENDIF
  UNLOCK AEGIS.MUTEX.ENTITYTRACKING.PLAYERS

RETURN VOID
