INCLUDE StdLib\player.l2c

DEFINE ARRAYLIST PLAYERS_AL 0
DEFINE SORTEDLIST PLAYERS_SL 0

GET_PLAYERS PLAYERS_AL
GET_PLAYERS PLAYERS_SL

PRINT_TEXT "writing to file..."

DEFINE FILEWRITER OUTFILE "players_file"

OUTFILE.WRITE PLAYERS_AL.COUNT

FOREACH P Player PLAYERS_AL
	PRINT_TEXT "writing entry..."
	cALL PLAYERS_AL.P.WRITE NULL 1 OUTFILE
NEXTEACH

OUTFILE.FLUSH
OUTFILE.CLOSE

PRINT_TEXT "done... pause"

PAUSE