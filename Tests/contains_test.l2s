DEFINE STRING S1 "hello world"
DEFINE STRING S2 "hell"
DEFINE STRING S3 "Hell"

PRINT_TEXT "test 1"
IF "S1.CONTAINS S2" == TRUE
	PRINT_TEXT "S1 (<&S1&>) CONTAINS S2 (<&S2&>)"
ELSE
	PRINT_TEXT "S1 (<&S1&>) DOES NOT CONTAIN S2 (<&S2&>)"
ENDIF

PRINT_TEXT "test 2"
IF "S1.CONTAINS S3" == TRUE
	PRINT_TEXT "S1 (<&S1&>) CONTAINS S3 (<&S3&>)"
ELSE
	PRINT_TEXT "S1 (<&S1&>) DOES NOT CONTAIN S3 (<&S3&>)"
ENDIF

PRINT_TEXT "test 3"
IF "S1.CONTAINS #$world" == TRUE
	PRINT_TEXT "S1 (<&S1&>) CONTAINS world"
ELSE
	PRINT_TEXT "S1 (<&S1&>) DOES NOT CONTAIN world"
ENDIF

PRINT_TEXT "test 4"
IF "S1.CONTAINS #$World" == TRUE
	PRINT_TEXT "S1 (<&S1&>) CONTAINS World"
ELSE
	PRINT_TEXT "S1 (<&S1&>) DOES NOT CONTAIN World"
ENDIF

PRINT_TEXT "done"

END_SCRIPT