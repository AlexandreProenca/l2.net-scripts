SET_EVENT "<&SCRIPTEVENT_CHAT&>" test.l2s chat_command  
 
DEFINE_GLOBAL INT CHAT_PRIVATE         2  
 
CALL chat_command
SLEEP 604800000 // 7 Days
PRINT_TEXT "End of Script"  
END_SCRIPT
 
FUNCTION chat_command  
 
DEFINE INT MESS 2
 IF  CHAT_PRIVATE == CHANNEL_PRIVATE
   PRINT_TEXT "equal <&CHANNEL_PRIVATE&> <&CHAT_PRIVATE&>"  
 ENDIF  
 IF  CHAT_PRIVATE == MESS
   PRINT_TEXT "equal <&CHANNEL_PRIVATE&> <&MESS&>"  
 ENDIF  
 
RETURN VOID  