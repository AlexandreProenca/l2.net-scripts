Window::PacketViewer.SET_NAME "Packet Trace"
Window::PacketViewer.SET_HTML ""
Window::PacketViewer.RUN_WINDOW

Init::Events VOID 0

WHILE ONE == ONE
  SLEEP 10000
WEND

END_SCRIPT


FUNCTION Init::GlobalVariables

  DEFINE_GLOBAL SORTEDLIST ServerPacket
  DEFINE_GLOBAL SORTEDLIST ExServerPacket
  DEFINE_GLOBAL SORTEDLIST ClientPacket
  DEFINE_GLOBAL SORTEDLIST ExClientPacket
  DEFINE_GLOBAL SORTEDLIST ServerFilterList
  DEFINE_GLOBAL SORTEDLIST ExServerFilterList
  DEFINE_GLOBAL SORTEDLIST ClientFilterList
  DEFINE_GLOBAL SORTEDLIST ExClientFilterList
  
  DEFINE_GLOBAL WINDOW Window::PacketViewer HTML  
  DEFINE_GLOBAL STRING HTML::Buffer
  
  Init::DataObjects VOID 0
  
RETURN VOID

FUNCTION Init::Events

  FOR Z 0 255 1
    SET_EVENT "<&SCRIPTEVENT_SERVERPACKET&>" "<&SYSTEM_CURRENTFILE&>" ServerPacket::Writer "<&Z&>"
  NEXT

  FOR Z 0 255 1
    SET_EVENT "<&SCRIPTEVENT_SERVERPACKETEX&>" "<&SYSTEM_CURRENTFILE&>" ServerExPacket::Writer "<&Z&>"
  NEXT

  FOR Z 0 255 1
    SET_EVENT "<&SCRIPTEVENT_CLIENTPACKET&>" "<&SYSTEM_CURRENTFILE&>" ClientPacket::Writer "<&Z&>"
  NEXT

  FOR Z 0 255 1
    SET_EVENT "<&SCRIPTEVENT_CLIENTPACKETEX&>" "<&SYSTEM_CURRENTFILE&>" ClientExPacket::Writer "<&Z&>"
  NEXT

RETURN VOID

FUNCTION DrawOverlay 1 _sBuffer
  
  HTML::Buffer = "#$<&_sBuffer&>" + HTML::Buffer
  
RETURN HTML::Buffer

FUNCTION ServerExPacket::Writer

  DEFINE INT PACKET_ID
  DEFINE INT BUF1
  DEFINE STRING BUF2
  DEFINE STRING BUF3
  PACKET.READ_BYTE NULL
  PACKET.READ_INT16 PACKET_ID
  
  LOCK MUTEX
    IF "ExServerFilterList.CONTAINS_KEY <&PACKET_ID&>" == TRUE
      PACKET_ID = PACKET_ID
    ELSE
      ResolveExServerPacket BUF2 1 PACKET_ID
      BUF2 = "#$<tr><td>(ES)</td><td align=\"center\"><&BUF2&></td><td>"
      PACKET.RESET_INDEX
      FOR Z 0 "<&PACKET.LENGTH&>" 1
        PACKET.READ_BYTE BUF1
        BUF3 = BUF1.GET_HEX
        BUF3 = "BUF3.SUBSTRING #I0 #I2"
        BUF2 = "#$<&BUF2&> <&BUF3&>"
      NEXT
      BUF2 = "#$<&BUF2&></td></tr>"
      DrawOverlay HTML::Buffer 1 BUF2
      Window::PacketViewer.SET_HTML "<html><body><font face=\"Lucida Console\"><table border=\"1\"><&HTML::Buffer&></table></font></body></html>"
    ENDIF
  UNLOCK MUTEX
RETURN VOID

