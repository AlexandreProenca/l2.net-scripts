FUNCTION ResolveExServerPacket 1 _iHeader

  DEFINE STRING HexVer
  DEFINE STRING PNAME
  
  HexVer = _iHeader.GET_HEX
  HexVer = HexVer.SUBSTRING #I0 #I2
  FOREACH X STRING ExServerPacket
    IF ExServerPacket.X.TO_UPPER == HexVer.TO_UPPER
      PNAME = "#$<&ExServerPacket.GET_KEY <&X&>&>"
    ENDIF
  NEXTEACH
  
RETURN PNAME

FUNCTION ResolveServerPacket 1 _iHeader

  DEFINE STRING HexVer
  DEFINE STRING PNAME

  HexVer = _iHeader.GET_HEX
  HexVer = HexVer.SUBSTRING #I0 #I2
  FOREACH X STRING ServerPacket
    IF ServerPacket.X.TO_UPPER == HexVer.TO_UPPER
      PNAME = "#$<&ServerPacket.GET_KEY <&X&>&>"
    ENDIF
  NEXTEACH
  
RETURN PNAME

FUNCTION ResolveExClientPacket 1 _iHeader

  DEFINE STRING HexVer
  DEFINE STRING PNAME
  
  HexVer = _iHeader.GET_HEX
  HexVer = HexVer.SUBSTRING #I0 #I2
  FOREACH X STRING ExClientPacket
    IF ExClientPacket.X.TO_UPPER == HexVer.TO_UPPER
      PNAME = "#$<&ExClientPacket.GET_KEY <&X&>&>"
    ENDIF
  NEXTEACH
  
RETURN PNAME

FUNCTION ResolveClientPacket 1 _iHeader

  DEFINE STRING HexVer
  DEFINE STRING PNAME

  HexVer = _iHeader.GET_HEX
  HexVer = HexVer.SUBSTRING #I0 #I2
  FOREACH X STRING ClientPacket
    IF ClientPacket.X.TO_UPPER == HexVer.TO_UPPER
      PNAME = "#$<&ClientPacket.GET_KEY <&X&>&>"
    ENDIF
  NEXTEACH
  
RETURN PNAME

