INCLUDE StdLib\inventory.l2c

DEFINE STRING ITEMNAME "Varnish"
DEFINE ARRAYLIST INVENTORY 0
DEFINE INT PLUSSLEEP 300000


GET_INVENTORY INVENTORY
GET_RAND PLUSSLEEP 300000 400000

FOREACH I Inventory INVENTORY
    IF INVENTORY.I.NAME == ITEMNAME
		LABEL HERE
		SAY_TEXT "<&CHANNEL_CLAN&>" "Type=1 ID=<&INVENTORY.I.ID&> Color=0 Underline=0 Title=<&INVENTORY.I.NAME&>"
		SLEEP "<&PLUSSLEEP&>"
		JUMP_TO_LABEL HERE
    ENDIF    
NEXTEACH

RETURN VOID
SCRIPT_END

