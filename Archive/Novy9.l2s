SET_EVENT "<&SCRIPTEVENT_SERVERPACKET&>" "<&SYSTEM_CURRENTFILE&>" NPCSpawn 12

while 0 == 0
sleep 1337
wend

FUNCTION NPCSpawn
DEFINE INT nothing
DEFINE INT NPCID
DEFINE INT header

PACKET.READ_BYTE nothing
PACKET.READ_INT32 NPCID
PACKET.READ_INT32 nothing
PACKET.READ_INT32 nothing
PACKET.READ_INT32 nothing
PACKET.READ_INT32 nothing
PACKET.READ_INT32 header

if header == 0
	PRINT_TEXT "HEADER 0 npc id=<&NPCID&>"
endif

RETURN VOID