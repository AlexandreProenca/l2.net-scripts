
	INCLUDE Include/EngineX.l2c
	DEFINE_GLOBAL ENGINEX ENGINEX 0

	PRINT_TEXT ""
	PRINT_TEXT "-------------------------------------------------"

	DEFINE ARRAYLIST LOL 0
	LOL.ADD "#i1"
	LOL.ADD "#$rofl"

	DEFINE INT CONTAINS 0

	ENGINEX.LISTS.CONTAINS CONTAINS 2 LOL #i1
	PRINT_TEXT "<&CONTAINS&>"

	ENGINEX.LISTS.CONTAINS CONTAINS 2 LOL #i2
	PRINT_TEXT "<&CONTAINS&>"

	ENGINEX.LISTS.CONTAINS CONTAINS 2 LOL #$rofl
	PRINT_TEXT "<&CONTAINS&>"

	DEFINE STRING ROFL "rofl"
	ENGINEX.LISTS.CONTAINS CONTAINS 2 LOL ROFL
	PRINT_TEXT "<&CONTAINS&>"

	LOL.ADD ROFL
	ENGINEX.LISTS.CONTAINS CONTAINS 2 LOL ROFL
	PRINT_TEXT "<&CONTAINS&>"

	PRINT_TEXT "-------------------------------------------------"
	PRINT_TEXT ""

END_SCRIPT
