	PRINT_TEXT "date: <&TIMEDATA.#$DAY_FORMATED&>.<&TIMEDATA.#$MONTH_FORMATED&>.<&TIMEDATA.#$YEAR&> (<&TIMEDATA.#$DAY_NAME&>) (<&TIMEDATA.#$MONTH_NAME&>)"
	PRINT_TEXT "time: <&TIMEDATA.#$HOUR_FORMATED&>:<&TIMEDATA.#$MINUTE_FORMATED&>:<&TIMEDATA.#$SECOND_FORMATED&>"
	PRINT_TEXT "week: <&TIMEDATA.#$WEEK&>"

