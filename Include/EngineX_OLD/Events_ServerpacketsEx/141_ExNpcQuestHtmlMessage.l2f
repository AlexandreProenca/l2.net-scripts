
FUNCTION READ 1 PACKET

	DEFINE INT PACKET_TYPE 0
	DEFINE INT PACKET_TYPE_EX 0
	DEFINE INT UNKNOWN 0
   DEFINE STRING HTML ""

	PACKET.READ_BYTE PACKET_TYPE
	PACKET.READ_INT16 PACKET_TYPE_EX
	PACKET.READ_INT32 UNKNOWN
   PACKET.READ_STRING HTML

   DEFINE SORTEDLIST PACKET_DATA 0
   PACKET_DATA.ADD PACKET_TYPE "PACKET_TYPE"
   PACKET_DATA.ADD PACKET_TYPE_EX "PACKET_TYPE_EX"
   PACKET_DATA.ADD HTML "HTML"

   PACKET_DATA.ADD PACKET "__PACKET"
RETURN PACKET_DATA
