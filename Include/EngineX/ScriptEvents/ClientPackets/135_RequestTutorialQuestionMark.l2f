
FUNCTION READ 1 PACKET

   PACKET.RESET_INDEX
	PACKET.READ_BYTE VOID

   DEFINE SORTEDLIST DATA 0
   DATA.ADD NULL "MARK_ID"

   PACKET.READ_INT32 DATA.#$MARK_ID

RETURN DATA

FUNCTION WRITE 1 DATA

   DEFINE BYTEBUFFER PACKET 19999
	PACKET.WRITE_BYTE 135

   IF ( "DATA.CONTAINS_KEY MARK_ID" == TRUE )
      PACKET.WRITE_INT32 DATA.#$MARK_ID
   ELSE
      PACKET.WRITE_INT32 ZERO
   ENDIF

   PACKET.TRIM_TO_INDEX
RETURN PACKET
