FUNCTION CreatureSay  
	DEFINE INT PACKET_ID                                                                                   
	DEFINE INT OBJ_ID
	DEFINE INT TEXT_TYPE
	DEFINE INT C_ID
	DEFINE INT M_ID 
	DEFINE STRING C_NAME
	DEFINE STRING TEXT

	DEFINE BYTEBUFFER TMP_PACKET 9999

	PACKET.READ_BYTE PACKET_ID
	PACKET.READ_INT32 OBJ_ID
	PACKET.READ_INT32 TEXT_TYPE
	
	TMP_PACKET.WRITE_BYTE PACKET_ID
	TMP_PACKET.WRITE_INT32 OBJ_ID
	TMP_PACKET.WRITE_INT32 TEXT_TYPE	
	
	IF PACKET.COUNT == 16
		PACKET.READ_INT32 C_ID
		PACKET.READ_INT32 M_ID	
		
		TMP_PACKET.WRITE_INT32 C_ID
		TMP_PACKET.WRITE_INT32 -1
		TMP_PACKET.WRITE_INT32 M_ID	
	ELSE
	 	PACKET.READ_STRING C_NAME
	 	PACKET.READ_STRING TEXT
	 	
	 	TMP_PACKET.WRITE_STRING C_NAME
	 	TMP_PACKET.WRITE_INT32 -1
	 	TMP_PACKET.WRITE_STRING TEXT
	ENDIF
	
	TMP_PACKET.TRIM_TO_INDEX
	//INJECTBB_CLIENT TMP_PACKET 
RETURN VOID

