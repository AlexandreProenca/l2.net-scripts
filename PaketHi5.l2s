FUNCTION ADD_TO_LIST 3 POSITION VALUE LIST
	DEFINE ARRAYLIST TMP_ARRAYLIST
	FOR A 0 "<&LIST.COUNT&>" 1
		IF A == POSITION
			TMP_ARRAYLIST.ADD VALUE
		ENDIF
		TMP_ARRAYLIST.ADD LIST.A
	NEXT
RETURN TMP_ARRAYLIST

FUNCTION UserInfo
	DEFINE SORTEDLIST USER_INFO
	CALL_EXTERN "DLIB\Packets\ReadUserInfo.l2s" DLIB::Packets::ReadUserInfo USER_INFO 1 PACKET
	DEFINE BYTEBUFFER TMP_PACKET
	CALL_EXTERN "DLIB\Packets\WriteUserInfo.l2s" DLIB::Packets::WriteUserInfo TMP_PACKET 1 USER_INFO
	INJECTBB_CLIENT TMP_PACKET
RETURN VOID

FUNCTION NpcSay

	DEFINE INT PACKET_ID
	DEFINE INT TEXT_TYPE
	DEFINE INT NPC_ID
	DEFINE INT NPC_STR
	
	PACKET.READ_BYTE PACKET_ID
	PACKET.READ_INT32 TEXT_TYPE
	PACKET.READ_INT32 NPC_ID
	PACKET.READ_INT32 NPC_STR
	PACKET.RESET_INDEX
	PACKET.WRITE_BYTE PACKET_ID
	PACKET.WRITE_INT32 TEXT_TYPE
	PACKET.WRITE_INT32 NPC_ID 
	IF NPC_STR == 0
		NPC_STR = -1
	ENDIF
	PACKET.WRITE_INT32 NPC_STR
//	INJECTBB_CLIENT PACKET
RETURN VOID

