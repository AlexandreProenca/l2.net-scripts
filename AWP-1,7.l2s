function oly_arena_spr
define int area_dist 0
define int area_temp 0
define int area_con 0
define int temp_pla 0
define int temp_ok 0


for a 0 "<&oly_arena.count&>" 1
DISTANCE area_dist CHAR_X CHAR_Y CHAR_Z oly_arena.a.x oly_arena.a.y oly_arena.a.z
if area_temp != 0
	if area_dist < area_temp
		area_temp = area_dist
		area_con = a.clone
	endif
else
area_temp = area_dist
area_con = a.clone
endif
next

area_str = oly_arena.GET_KEY area_con


DISTANCE area_dist CHAR_X CHAR_Y CHAR_Z "oly_arena.#$<&area_str&>.st1_x" "oly_arena.#$<&area_str&>.st1_y" "oly_arena.#$<&area_str&>.st1_z"
