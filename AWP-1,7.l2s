	endif
	if "byp21.CONTAINS temp_byp" == true
		heal_tit = 1
		call refr_chars
		call okno_adv_ig
		jump_to_label byp_end
	endif
	if "temp_byp.CONTAINS #$l2net_agh_id" == true
		max_cou2 = temp_byp.count - min_cou2
		if temp_byp.count > 13
		kic2 = temp_byp.SUBSTRING min_cou2 max_cou2
		id_agha = kic2
		call write_user_info
		call okno_adv_ig
		endif
	endif


	if "temp_byp.CONTAINS #$l2net_page" == true
		pag_c = temp_byp.count - pag_in
		kic2 = temp_byp.SUBSTRING pag_in pag_c
		call okno_lista void 1 kic2
	endif

	if "temp_byp.CONTAINS #$l2net_next" == true
		pag_c = temp_byp.count - pag_in
		kic2 = temp_byp.SUBSTRING pag_in pag_c
		pag_c = ppl_arr.count
		pag_in = pag_c / 15
		pag_in = pag_in + 1
		if kic2 < pag_in
			kic2 = kic2 + 1
			call okno_lista void 1 kic2
		else
			call okno_lista void 1 pag_in
		endif

	endif
	if "temp_byp.CONTAINS #$l2net_back" == true
		pag_c = temp_byp.count - pag_in
		kic2 = temp_byp.SUBSTRING pag_in pag_c
		if kic2 > 1
			kic2 = kic2 - 1
			call okno_lista void 1 kic2
		else
			call okno_lista void 1 1
		endif

	endif
	if "byp28.CONTAINS temp_byp" == true
		Oly_mod = 0
		arena_jest = 0
		oly_enemy_id = 0
		oly_enemy_skils.clear
		call okno_adv_ig
		jump_to_label byp_end
	endif
	if "byp29.CONTAINS temp_byp" == true
		Oly_mod = 1
		arena_jest = 0
		oly_enemy_id = 0
		oly_enemy_skils.clear
		auto_pots = 0
		call okno_ig
		call okno_adv_ig
		jump_to_label byp_end
	endif

else


injectbb packet

endif


label byp_end

return void

