
FUNCTION READ 1 PACKET

	DEFINE INT PACKET_TYPE 0
   DEFINE INT FAILED 0
   DEFINE INT CRYTAL_ID 0
   DEFINE INT CRYTAL_COUNT 0

	PACKET.READ_BYTE PACKET_TYPE
   PACKET.READ_INT32 FAILED
   PACKET.READ_INT32 CRYTAL_ID
   PACKET.READ_INT32 CRYTAL_COUNT

   DEFINE SORTEDLIST PACKET_DATA 0
   PACKET_DATA.ADD PACKET_TYPE "PACKET_TYPE"
   PACKET_DATA.ADD FAILED "FAILED"
   PACKET_DATA.ADD CRYTAL_ID "CRYTAL_ID"
   PACKET_DATA.ADD CRYTAL_COUNT "CRYTAL_COUNT"

RETURN PACKET_DATA
