
//-------------------------------------------------------------------------------------
CLASS SIZE NULL
//-------------------------------------------------------------------------------------

   VAR_START
		PUBLIC INT WIDTH 0
		PUBLIC INT HEIGHT 0
   VAR_END

//-------------------------------------------------------------------------------------

   PUBLIC = 2 WIDTH HEIGHT
      IF ( ( WIDTH.TYPE != INT && WIDTH.TYPE != DOUBLE ) || ( HEIGHT.TYPE != INT && HEIGHT.TYPE != DOUBLE ) )
         RETURN FALSE
      ENDIF
      THIS.WIDTH = WIDTH + ZERO
      THIS.HEIGHT = HEIGHT + ZERO
   RETURN TRUE

//-------------------------------------------------------------------------------------
END_CLASS
//-------------------------------------------------------------------------------------
