
//-------------------------------------------------------------------------------------
CLASS VECTOR3 NULL
//-------------------------------------------------------------------------------------

   VAR_START
		PUBLIC DOUBLE X 0
		PUBLIC DOUBLE Y 0
		PUBLIC DOUBLE Z 0
   VAR_END

//-------------------------------------------------------------------------------------

   PUBLIC = 3 X Y Z
      IF ( ( ( X.TYPE != INT && X.TYPE != DOUBLE ) || ( Y.TYPE != INT && Y.TYPE != DOUBLE ) ) || ( Z.TYPE != INT && Z.TYPE != DOUBLE ) )
         RETURN FALSE
      ENDIF
      THIS.X = X + ZERO
      THIS.Y = Y + ZERO
      THIS.Z = Z + ZERO
   RETURN TRUE

//-------------------------------------------------------------------------------------
END_CLASS
//-------------------------------------------------------------------------------------
