
//-------------------------------------------------------------------------------------
CLASS VECTOR2 NULL
//-------------------------------------------------------------------------------------

   VAR_START
		PUBLIC DOUBLE X 0
		PUBLIC DOUBLE Y 0
   VAR_END

//-------------------------------------------------------------------------------------

   PUBLIC = 2 X Y
      IF ( ( X.TYPE != INT && X.TYPE != DOUBLE ) || ( Y.TYPE != INT && Y.TYPE != DOUBLE ) )
         RETURN FALSE
      ENDIF
      THIS.X = X + ZERO
      THIS.Y = Y + ZERO
   RETURN TRUE

//-------------------------------------------------------------------------------------
END_CLASS
//-------------------------------------------------------------------------------------
